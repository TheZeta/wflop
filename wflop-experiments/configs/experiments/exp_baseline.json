{
  "experimentName": "wdga_vs_baselines",
  "runs": 1,
  "problems": [
    { "id": "ws1_10x10_t20", "path": "configs/problems/ws1_10x10_t20.json" },
    { "id": "ws1_10x10_t40", "path": "configs/problems/ws1_10x10_t40.json" }
  ],
  "algorithms": [
    { "id": "ga",     "path": "configs/algorithms/ga.json" },
    { "id": "sa",     "path": "configs/algorithms/sa.json" },
    { "id": "de",     "path": "configs/algorithms/de.json" },
    { "id": "lshade", "path": "configs/algorithms/lshade.json" },
    { "id": "fode",   "path": "configs/algorithms/fode.json" },
    { "id": "wdga",   "path": "configs/algorithms/wdga.json" }
  ],
  "listeners": [
    "convergence",
    "progressBar"
  ],
  "processors": [
    {
      "id": "convergence",
      "params": {
        "mode": "ITERATION",
        "aggregation": "NONE",
        "outputPath": "wflop-experiments/results/convergence"
      }
    },
    {
      "id": "average-best",
      "params": {
        "outputPath": "wflop-experiments/results/average-best"
      }
    }
  ]
}
